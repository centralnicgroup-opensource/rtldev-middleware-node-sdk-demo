{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ES2017",
    "outDir": "./dist",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true
  },
  "lib": [
    "ES2017"
  ]
}
